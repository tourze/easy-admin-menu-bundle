{
  "name": "tourze/easy-admin-menu-bundle",
  "description": "菜单模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "easycorp/easyadmin-bundle": "^4",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\EasyAdminMenuBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\EasyAdminMenuBundle\\Tests\\": "tests"
    }
  }
}